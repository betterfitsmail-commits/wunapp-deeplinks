{
  "rewrites": [
    {
      "source": "/stripe-return",
      "destination": "/stripe-return.html"
    },
    {
      "source": "/event/:id",
      "destination": "/event.html"
    },
    {
      "source": "/music/:id",
      "destination": "/music.html"
    },
    {
      "source": "/profile/:username",
      "destination": "/profile.html"
    },
    {
      "source": "/post/:id",
      "destination": "/post.html"
    },
    {
      "source": "/reset-password",
      "destination": "/reset-password.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
